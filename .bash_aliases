# Navigating
alias ..='cd ..'
alias ...='cd ../..'
alias ....='cd ../../..'
alias home='cd ~'

alias restart='sudo shutdown -r now'

# Manage packages
alias agi='sudo apt-get install'
alias agr='sudo apt-get autoremove'
alias agu='sudo apt-get update'

# EOS command
alias eos='cd ~/eos'
alias eosny='cleos -u http://api.eosnewyork.io'
